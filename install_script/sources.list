# deb http://ftp.fr.debian.org/debian/ stable main

deb http://ftp.fr.debian.org/debian/ stable main
deb-src http://ftp.fr.debian.org/debian/ stable main

deb http://security.debian.org/debian-security stable/updates main
deb-src http://security.debian.org/debian-security stable/updates main

# stable-updates, previously known as 'volatile'
deb http://ftp.fr.debian.org/debian/ stable-updates main
deb-src http://ftp.fr.debian.org/debian/ stable-updates main

deb http://ftp.fr.debian.org/debian/ stable contrib non-free
deb-src http://ftp.fr.debian.org/debian/ stable contrib non-free

deb http://ftp.fr.debian.org/debian/ stretch-backports contrib non-free
deb-src http://ftp.fr.debian.org/debian/ stretch-backports contrib non-free

